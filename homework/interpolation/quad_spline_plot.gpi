set terminal png
set output "$@"
set key outside
set tics in
set grid
set xlabel "$x$"
set ylabel "$S_i(x)$"
set title "$\cos(x)$ quadratic spline"
set output "quad_spline_plot.png"
plot "quad_spline_output.txt" using 1:2 with line lw 3 col PineGreen title "Interpolant" \
,"quad_spline_output.txt" using 1:3 with line lw 1 col SeaGreen title "Interpolant by GSL" \
, "cos_tabulated.txt" using 1:2 with points col Black pointtype 2 title "Tabulated data"
